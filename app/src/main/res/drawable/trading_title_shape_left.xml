<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners
                android:topLeftRadius="30dp"
                android:bottomLeftRadius="30dp"
                />
            <solid android:color="@color/trading_title_backgroud_normal"/>
            <stroke android:width="1dp"
                android:color="@color/trading_title_border"
                />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners
                android:topLeftRadius="30dp"
                android:bottomLeftRadius="30dp"
                />
            <solid android:color="@color/trading_title_backgroud_selector"/>
            <stroke android:width="1dp"
                android:color="@color/trading_title_border"
                />
        </shape>
    </item>
    <item android:drawable="@color/trading_title_backgroud_selector"/>
</selector>
