    android:orientation="vertical"
    android:background="@color/global_deep_background"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <!--交易页面标题栏-->
        <include layout="@layout/trading_title"
            android:id="@+id/llt_trading_title"
            />

        <FrameLayout
            android:id="@+id/fragment_Trading"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/llt_trading_title"
            android:background="#000000"
            >

        </FrameLayout>



            <!--++++++++++++++++++++++++++++++++++++-->




    </RelativeLayout>
</LinearLayout>
